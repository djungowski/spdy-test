            containerId:'paidlisting-boxes',
            containerDisabled:false        };
            query_zones['full-banner']  = {
            zoneId:'202',
            containerId:'',
            containerDisabled:false        };
        // filter out only those zones that are actually used in the current page
    window.OA_zones = Cxo_Adtech.AdServer_Spc_OpenX.getZones(query_zones);
    if (typeof(OA_output) != 'undefined') var OA_temp = OA_output;
</script>
