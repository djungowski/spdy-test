    zoneid: 'footerad'
}), []);
//]]>--></script></div><script type="text/javascript">
    new Cxo_Adtech_Decorator_Anzeige({"zoneid":"footerad","position":"before"}).run();
</script>    <script type="text/javascript">
        Cxo_Adtech.placeAd('footerad');
    </script>
<div id="async-full-banner" style="display:none;" ><script type='text/javascript'><!--//<![CDATA[
Cxo_Adtech.callAdServer('full-banner', new Cxo_Adtech_AdServer_Spc_OpenX({
    zoneid: 'full-banner'
}), []);
//]]>--></script></div><script type="text/javascript">
Cxo_Adtech.registerRule(
    'full-banner',
    Cxo_Adtech_Rule_ShowAsyncOnContent,
    {"zoneId":"full-banner"});
</script><script type="text/javascript">
    new Cxo_Adtech_Decorator_Anzeige({"zoneid":"full-banner","position":"before"}).run();
</script>    <script type="text/javascript">
        Cxo_Adtech.placeAd('full-banner');
    </script>
