                .write('<script> function CxoAdtechAdServerTfsputOver(){(function(){var list, parent, cur;list = document.getElementsByTagName("script");while(typeof(list[0]) !== "undefined"){ cur = list[0];parent = cur.parentNode;parent.removeChild(cur);}}());var iframeContent = document.getElementsByTagName("body")[0].innerHTML;top.document.getElementById("top-banner").innerHTML+=iframeContent;}</scr'+'ipt>');
