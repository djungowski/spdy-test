if("undefined"==typeof(EFWS)||!EFWS){var EFWS={}}EFWS.Script={loadScript:function(a,b){EFWS.Script.loadScriptDomElement(a,b)},loadScripts:function(c,e){var d=c.length;var a=false;for(var b=0;b<d;b++){if(EFWS.Script.differentDomain(c[b])){a=true;break}}var f=EFWS.Script.loadScriptXhrInjection;if(a){if(-1!=navigator.userAgent.indexOf("Firefox")||-1!=navigator.userAgent.indexOf("Opera")){f=EFWS.Script.loadScriptDomElement}else{f=EFWS.Script.loadScriptDocWrite}}for(var b=0;b<d;b++){f(c[b],(b+1==d?e:null),true)}},differentDomain:function(b){if(0===b.indexOf("http://")||0===b.indexOf("https://")){var a=document.location.protocol+"://"+document.location.host+"/";return(0!==b.indexOf(a))}return false},loadScriptDomElement:function(b,c){var a=document.createElement("script");a.src=b;if(c){a.onloadDone=false;a.onload=function(){a.onloadDone=true;if(null!==c){c()}};a.onreadystatechange=function(){if("loaded"===a.readyState&&!a.onloadDone){a.onloadDone=true;a.onload()}}}document.getElementsByTagName("head")[0].appendChild(a)},loadScriptDocWrite:function(a,b){document.write('<script src="'+a+'" type="text/javascript"><\/script>');if(b){EFWS.addHandler(window,"load",b)}},queuedScripts:new Array(),loadScriptXhrInjection:function(d,e,f){var b=EFWS.Script.queuedScripts.length;if(f){var a={response:null,onload:e,done:false};EFWS.Script.queuedScripts[b]=a}var c=EFWS.Script.getXHRObject();c.onreadystatechange=function(){if(c.readyState==4){if(f){EFWS.Script.queuedScripts[b].response=c.responseText;EFWS.Script.injectScripts()}else{var g=document.createElement("script");document.getElementsByTagName("head")[0].appendChild(g);g.text=c.responseText;if(e){e()}}}};c.open("GET",d,true);c.send("")},injectScripts:function(){var b=EFWS.Script.queuedScripts.length;for(var c=0;c<b;c++){var a=EFWS.Script.queuedScripts[c];if(!a.done){if(a.response===null){break}else{var d=document.createElement("script");document.getElementsByTagName("head")[0].appendChild(d);d.text=a.response;if(a.onload){a.onload(