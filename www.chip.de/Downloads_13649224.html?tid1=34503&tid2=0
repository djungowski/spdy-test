    targets = false,
    target = false,
    g_target = false,
    saBL = d.getElementById('story-ad-beitragslisten');

if (saBL && d.getElementsByName('beitragsliste-link').length > 0) {
    target = d.getElementsByName('beitragsliste-link')[3].parentNode;
    if (cxo_adtech_channel == '30593') target = target.parentNode;
