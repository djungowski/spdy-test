        g_target = targets[9].parentNode;
    }
}
if (target) target.parentNode.insertBefore(saBL,target);

if (g_target) {
    var div = document.createElement('div');
    div.className = 'google_ads_v3 storyad';
    div.style.margin = '0 auto 15px';
    Cxo_Adtech.google.addRequest({
      container:div,
      max_num_ads:1,
      channel_suffix:'storyad',
      callback:function(){
        g_target.parentNode.insertBefore(div, g_target);
      }
    });
}
</script>
</div>
