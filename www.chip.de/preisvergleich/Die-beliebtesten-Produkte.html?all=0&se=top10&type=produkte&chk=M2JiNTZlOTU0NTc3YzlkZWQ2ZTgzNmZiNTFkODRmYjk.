<script type="text/javascript" language="JavaScript">
// <![CDATA[

function cxo_tracking (){
    this.name = 'cxo_tracking';

    this.subclasses = new Array();

    this.anmelden = function ( superclass ) {
        superclass.subclasses.push( this )
    }

    this.image_id = '';

        this.refresh = function () {
        //        console.log('hier meldet sich ' + this.image_id + ' mit einem refresh');
                try {
                        if(document.getElementById( this.image_id ) == null) {
                             // console.error ('noImage');
                        }

                        // Randomwert von ivw wird neu gesetzt - Randomwert (&d=...) muss der letzte Parameter sein, da alles hinter &d= abgeschnitten wird!
                        var pixelSrc = document.getElementById( this.image_id ).src;
                        var dPos = pixelSrc.indexOf("&d=");
                        if(dPos != -1) {
                                pixelSrc = pixelSrc.substring(0, dPos) + "&d=" + Math.random();
                        } else {
                                pixelSrc = pixelSrc + "&d=" + Math.random();
                        }
                        var newPixel = document.createElement("img");
                        newPixel.id = this.image_id;
                        newPixel.src = pixelSrc;
                        var oldPixel = document.getElementById( this.image_id );
                        oldPixel.parentNode.replaceChild(newPixel, oldPixel);
               } catch(e) {
                         // console.error(e);
               }
        }
}


cxo_tracking_dispatcher = function () {

    this.inheritFrom = cxo_tracking;
    this.inheritFrom();

    this.name = 'cxo_tracking_dispatcher';

    this.refresh = function () {
        for (var i in this.subclasses) {
            try {
                this.subclasses[i].refresh();
            } catch (e) {
//                console.error(e);
            }
        }
    }
}

tracking_obj = new cxo_tracking_dispatcher();
tracking_ivw = function () {

    this.inheritFrom = cxo_tracking;
    this.inheritFrom();

    this.name = 'tracking_ivw';

    this.image_id = 'ivw_241326701';
}
try {
    tracking_ivw_obj = new tracking_ivw ();
    tracking_ivw_obj.anmelden( tracking_obj );
} catch (e) {
//    console.error(e);
}
tracking_omniture = function () {

    this.inheritFrom = cxo_tracking;
    this.inheritFrom();

    this.name = 'tracking_omniture';

    this.refresh = function () {
//        console.log('hier meldet sich tracking_omniture.refresh');
        var s_code="";
        var s_code=somtr.t();
    }
}
try {
    tracking_omniture_obj = new tracking_omniture ();
    tracking_omniture_obj.anmelden( tracking_obj );
} catch (e) {
//    console.error(e);
}
