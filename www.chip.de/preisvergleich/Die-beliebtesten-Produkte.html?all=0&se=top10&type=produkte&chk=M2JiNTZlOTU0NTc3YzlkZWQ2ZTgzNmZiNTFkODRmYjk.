    function Cxo_Adtech_AdServer_TFS(config)
    {
               
        if('undefined' != config.tfminstantiframe && config.tfminstantiframe == true){
            return function() {  
                createSrcLessIFrame = function(){ 
                    var iframe = document.createElement('iframe');
                    iframe.style.display = 'block';
                    document.getElementsByTagName('head')[0]
                    .appendChild(iframe);
                    return iframe; 
                }
                var iframeToHead = createSrcLessIFrame();
                iframeToHead.contentWindow.document.open();
                iframeToHead.contentWindow.document
                .write('<!doctype html><html><head>');
                iframeToHead.contentWindow.document
                .write('<script> function CxoAdtechAdServerTfsputOver(){(function(){var list, parent, cur;list = document.getElementsByTagName("script");while(typeof(list[0]) !== "undefined"){ cur = list[0];parent = cur.parentNode;parent.removeChild(cur);}}());var iframeContent = document.getElementsByTagName("body")[0].innerHTML;top.document.getElementById("top-banner").innerHTML+=iframeContent;}</scr'+'ipt>');
                iframeToHead.contentWindow.document
                .write('</head>');
                iframeToHead.contentWindow.document
                .write('<body onload="CxoAdtechAdServerTfsputOver(); ">');
                iframeToHead.contentWindow.document 
                .write('<script type="text/javascript">var adsc_rubrik = top.adsc_rubrik;var adsc_sb = top.adsc_sb;var adsc_mr = top.adsc_mr;var adsc_sk = top.adsc_sk;var adsc_sc = top.adsc_sc;var adsc_agof = top.adsc_agof;var adsc_keyword = top.adsc_keyword;var adsc_percentages = top.adsc_percentages;var adsc_sadwidth = top.adsc_sadwidth;var adsc_sadheight = top.adsc_sadheight;var adsc_cadwidth = top.adsc_cadwidth;var adsc_cadheight = top.adsc_cadheight;var adsc_popupwidth = top.adsc_popupwidth;var adsc_popupheight = top.adsc_popupheight;var adsc_skyscraperwidth = top.adsc_skyscraperwidth;var adsc_skyscraperheight = top.adsc_skyscraperheight;var adsc_layout = top.adsc_layout;var adsc_brand = top.adsc_brand;var adsc_product = top.adsc_product;var adsc_ressort = top.adsc_ressort;var adsc_rubrik_int = top.adsc_rubrik_int;var adsc_document_uri = top.adsc_document_uri;</scr'+'ipt>');
                iframeToHead.contentWindow.document
                .write('<script type="text/javascript" src="http://a.tfag.de/js_ng/js_ng_chip_0300.js"></scr'+'ipt>');
                iframeToHead.contentWindow.document
                .write('<script language="javascript"> document.write(ads_gettag("BANNER",728,90,100,"js")); </scr'+'ipt>');
                iframeToHead.contentWindow.document 
                .write('</bo'+'dy></html>');
                iframeToHead.contentWindow.document.close();
 
                                    if (typeof(console) == 'object') console.info("call TFMINSTANTIFRAME");
                    return '';
                  
                
            }
        } else {

            return function() {
                var adsource = ads_gettag(config.zoneid, config.width, config.height, 100);
                return adsource;
            }
 
        }
    }
